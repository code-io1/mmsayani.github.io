<ul id="page_links">
{{~ it.navigation:nav }}
  <li>
    {% if page.page_name == '{{=nav.page}}' %}
      <span class="current_page">{{=nav.linkText}}</span>
    {% else %}
      <a href="/{{?it.site}}{{=it.site}}/{{?}}{{?!nav.home}}{{=nav.page}}.html{{?}}" title="{{=nav.linkText}}">{{=nav.linkText}}</a>
    {% endif %}
  </li>
{{~}}
</ul>
